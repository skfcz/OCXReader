{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/master/docs/vcpkg.schema.json",
  "name": "ocxreader",
  "version": "0.0.1",
  "license": "LGPL-2.1",
  "description": [
    "OCXReader is a tool to read and parse data from OCX files and export them",
    "to different formats such as STEP, SHIPXML, glTF, XCAF-XML or XCAF-XFB."
  ],
  "builtin-baseline": "4b6bce313ef917b9635e9287d06225743e242888",
  "dependencies": [
    {
      "name": "spdlog",
      "version>=": "1.11.0"
    },
    {
      "name": "opencascade",
      "version>=": "7.6.2#1",
      "platform": "!(uwp | osx | arm)"
    },
    {
      "name": "gtest",
      "version>=": "1.12.1"
    },
    {
      "name": "boost-system",
      "version>=": "1.80.0#1"
    },
    {
      "name": "boost-filesystem",
      "version>=": "1.80.0#1"
    },
    {
      "name": "boost-program-options",
      "version>=": "1.80.0#1"
    },
    {
      "name": "boost-property-tree",
      "version>=": "1.80.0#1"
    }
  ]
}
